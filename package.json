{
  "name": "print3DClient",
  "version": "0.1.4",
  "private": false,
  "scripts": {
    "serve": "clear && export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider &&vue-cli-service build",
    "lint": "vue-cli-service lint",
    "vue-i18n-extract-add": "vue-i18n-extract report --add  --vueFiles './src/components/**/*.?(ts|vue)' --languageFiles './src/locales/*.?(json|yml|yaml|js)'",
    "vue-i18n-extract-remove": "vue-i18n-extract report --remove  --vueFiles './src/components/**/*.?(ts|vue)' --languageFiles './src/locales/*.?(json|yml|yaml|js)'"

  },
  "dependencies": {
    "axios": "^0.26.0",
    "core-js": "^3.6.5",
    "echarts": "^5.3.0",
    "echarts-for-vue": "^1.4.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-codemirror": "^4.0.6",
    "vue-echarts": "^6.0.2",
    "vue-i18n": "^8.27.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/composition-api": "^1.4.6",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "typescript": "~4.1.5",
    "vue-cli-plugin-vuetify": "~2.4.5",
    "vue-i18n-extract": "^2.0.4",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  }
